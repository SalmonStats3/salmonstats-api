{
  "name": "salmonstats",
  "version": "0.0.1",
  "description": "Salmon Stats + Next Wave Server",
  "author": "tkgling",
  "private": false,
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "dotenv -e .env.local -- nest start --watch",
    "start:prod": "dotenv -e .env.prod -- nest start --watch",
    "studio:dev": "dotenv -e .env.local -- yarn prisma studio",
    "studio:prod": "dotenv -e .env.prod -- yarn prisma studio",
    "migrate:dev": "dotenv -e .env.local -- yarn prisma migrate reset && yarn prisma migrate dev --name init",
    "migrate:prod": "dotenv -e .env.prod -- yarn prisma migrate dev --name init",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/axios": "^0.0.8",
    "@nestjs/common": "^8.4.7",
    "@nestjs/config": "^2.1.0",
    "@nestjs/core": "^8.4.7",
    "@nestjs/passport": "^9.0.0",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/swagger": "^5.2.1",
    "@prisma/client": "^4.2.0",
    "@types/cache-manager": "^4.0.1",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "base64url": "^3.0.1",
    "cache-manager": "^4.0.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "dayjs": "^1.11.2",
    "form-data": "^4.0.0",
    "mathjs": "^10.6.4",
    "matrix-transpose": "^1.0.2",
    "node-fetch": "^3.2.5",
    "passport": "^0.6.0",
    "redoc-cli": "^0.13.16",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "snakecase-keys": "^5.4.2",
    "swagger-ui-express": "^4.4.0"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.2.8",
    "@nestjs/schematics": "^8.0.11",
    "@nestjs/testing": "^8.4.6",
    "@types/express": "^4.17.13",
    "@types/jest": "28.1.4",
    "@types/node": "^18.0.1",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.30.4",
    "@typescript-eslint/parser": "^5.30.4",
    "dotenv-cli": "^5.1.0",
    "eslint": "^8.19.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-jest": "^26.5.3",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^28.1.2",
    "prettier": "^2.3.2",
    "prisma": "^4.7.1",
    "source-map-support": "^0.5.20",
    "supertest": "^6.2.4",
    "ts-jest": "^28.0.3",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.8.2",
    "tsconfig-paths": "^4.0.0",
    "typescript": "^4.7.2"
  },
  "engines": {
    "node": "16.15.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
